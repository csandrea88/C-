
<h1><Results:</h1>
<br><br><br><br>
<div>
    @{
        
        string errormsg = "false";
        
        foreach(var error in ViewBag.errors) {
            
            if(@error.Errors.Count > 0)
            {
                <p>@error.Errors[0].ErrorMessage</p>
                errormsg = "true";
            }
        }

        if (errormsg == "false") {
                <p>Successful</p>
        }
    }           
    
</div>


